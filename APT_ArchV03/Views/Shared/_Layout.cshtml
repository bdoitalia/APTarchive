<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/bdofavicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/cssjqryUi")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryui")



    <link href="~/Content/style.css" rel="stylesheet" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Home Page", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("New", "Create", "Caws")</li>*@

                    @* <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
                <p class="nav navbar-text navbar-right">Hello, @User.Identity.Name!</p>
            </div>
        </div>

        <div class="nav navbar-default">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="@Url.Action("Index", "Home")" class="navbar-brand">
                        <img src="~/Content/img/logo.png" class="logo" title="title" alt="additional title" />
                    </a>

                    <ul class="nav navbar-nav">
                        <li class="inserisci">@Html.ActionLink("INSERISCI CAW", "Create")</li>
                    </ul>

                    <ul class="nav navbar-nav">
                        <li class="inserisci">@Html.ActionLink("CONTACT", "Create")</li>
                    </ul>



                </div>
            </div>
        </div>

    </div>

   
    





    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            @*codice footer*@
        </footer>
    </div>

    
    @RenderSection("scripts", required: false)
</body>
</html>
